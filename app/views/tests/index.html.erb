<p id="notice"><%= notice %></p>

<h1><%= t('.header') %></h1>

<table>
  <thead>
    <tr>
      <th colspan="4"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= test.questions.count %></td>
        <td><%= link_to t('.show'), test_path(test) %></td>
        <td><%= button_to t('.start'), start_test_path(test) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
